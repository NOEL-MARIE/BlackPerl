{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "scrollbar-width",
          "scrollbar-width: none"
        ]
      }
    ],
    "typescript-config/strict": "off",
    "typescript-config/consistent-casing": "off",
    "typescript-config/is-valid": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}